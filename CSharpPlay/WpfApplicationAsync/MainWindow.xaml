<Window x:Class="WpfApplicationAsync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button Content="Download RSS" Name="btnDownloadRss" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="497" Height="25" Click="btnDownloadRss_Click"/>
        <Button Content="Increase Count" Name="btnIncreaseCount" HorizontalAlignment="Left" Margin="10,40,0,0" VerticalAlignment="Top" Width="497" Height="25" Click="btnIncreaseCount_Click"/>
        <Label Content="Count" Name="lblCount" HorizontalAlignment="Left" Margin="5,108,0,0" VerticalAlignment="Top"/>
        <Label Content="CountValue" Name="lblCountValue" HorizontalAlignment="Left" Margin="48,108,0,0" VerticalAlignment="Top"/>
        <TextBlock Name="txtRss" HorizontalAlignment="Left" Margin="10,139,0,-21" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="201" Width="497"/>
        <Button Content="Login" Name="btnLogin" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="497" Height="24" Click="btnLogin_Click"/>

    </Grid>
</Window>
